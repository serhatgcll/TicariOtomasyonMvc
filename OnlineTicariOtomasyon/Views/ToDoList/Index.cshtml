<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="dist/img/AdminLTELogo.png" alt="AdminLTELogo" height="60" width="60">
        </div>

        <div class="card">
            <div class="card-header">
                <h3 class="card-title">
                    <i class="ion ion-clipboard mr-1"></i>
                    Şirket İçi Yapılacak İşler
                </h3>
                <div class="card-tools">
                    <ul class="pagination pagination-sm">
                        <li class="page-item"><a href="#" class="page-link">&laquo;</a></li>
                        <li class="page-item"><a href="#" class="page-link">1</a></li>
                        <li class="page-item"><a href="#" class="page-link">2</a></li>
                        <li class="page-item"><a href="#" class="page-link">3</a></li>
                        <li class="page-item"><a href="#" class="page-link">&raquo;</a></li>
                    </ul>
                </div>
            </div>

            <div class="card-body">
                <ul class="todo-list" data-widget="todo-list">
                    @foreach (var item in Model)
                    {


                        <li>

                            <span class="handle">
                                <i class="fas fa-ellipsis-v"></i>
                                <i class="fas fa-ellipsis-v"></i>
                            </span>

                            <div class="icheck-primary d-inline ml-2">
                                <input type="checkbox" value="" name="todo1" id="todoCheck1">
                                <label for="todoCheck1"></label>
                            </div>

                            <span class="text">@item.Description</span>

                            <small class="badge badge-danger"><i class="far fa-clock"></i></small>

                            <div class="tools">
                                <i class="fas fa-edit"></i>
                                <i class="fas fa-trash-o"></i>
                            </div>
                        </li>
                    }

                </ul>
            </div>

            <div class="card-footer clearfix">
                <button type="button" class="btn btn-primary float-right"><i class="fas fa-plus"></i> Yeni Bir İş Oluştur</button>
            </div>
        </div>



    </div>


    <aside class="control-sidebar control-sidebar-dark">
    </aside>

    </div>


    <script src="~/Web/AdminLte-3.2.0plugins/jquery/jquery.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/jquery-ui/jquery-ui.min.js"></script>

    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>

    <script src="~/Web/AdminLte-3.2.0plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/chart.js/Chart.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/sparklines/sparkline.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Web/AdminLte-3.2.0plugins/jqvmap/maps/jquery.vmap.usa.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/jquery-knob/jquery.knob.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/moment/moment.min.js"></script>
    <script src="~/Web/AdminLte-3.2.0plugins/daterangepicker/daterangepicker.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/summernote/summernote-bs4.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>

    <script src="~/Web/AdminLte-3.2.0dist/js/adminlte.js?v=3.2.0"></script>

    <script src="~/Web/AdminLte-3.2.0dist/js/demo.js"></script>

    <script src="~/Web/AdminLte-3.2.0dist/js/pages/dashboard.js"></script>
</body>
</html>
