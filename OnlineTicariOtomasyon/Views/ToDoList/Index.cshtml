    <link rel="stylesheet" href="~/Web/AdminLte-3.2.0plugins/jqvmap/jqvmap.min.css">

    <link rel="stylesheet" href="~/Web/AdminLte-3.2.0dist/css/adminlte.min.css?v=3.2.0">

    <link rel="stylesheet" href="~/Web/AdminLte-3.2.0plugins/overlayScrollbars/css/OverlayScrollbars.min.css">

    <link rel="stylesheet" href="~/Web/AdminLte-3.2.0plugins/daterangepicker/daterangepicker.css">

    <link rel="stylesheet" href="~/Web/AdminLte-3.2.0plugins/summernote/summernote-bs4.min.css">
