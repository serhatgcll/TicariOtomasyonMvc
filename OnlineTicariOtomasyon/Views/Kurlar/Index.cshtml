@using OnlineTicariOtomasyon.Models.Class
@model List<Currency>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
 <h3>Kur Son Güncelleme Zamanı:@ViewBag.result</h3>
<table class="table table-bordered">
<thead>
    <tr>
        <th scope="col">İsim</th>
        <th scope="col">Kod</th>
        <th scope="col">Alış Fiyatı</th>
        <th scope="col">Satış Fiyatı</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
        
            <td>@item.Name</td>
            <td>@item.Code</td>
            <td>@item.ForexBuying</td>
            <td>@item.ForexSelling</td>
            <td></td>
        </tr>
    }


</tbody>
</table>